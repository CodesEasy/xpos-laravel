{
    "name": "codeseasy/xpos-laravel",
    "description": "Instant public URLs for Laravel development - XPOS tunnel integration",
    "keywords": [
        "laravel",
        "tunnel",
        "xpos",
        "development",
        "localhost",
        "public-url",
        "share"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "CodesEasy",
            "email": "info@codeseasy.com",
            "homepage": "https://www.codeseasy.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "symfony/process": "^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "CodesEasy\\Xpos\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodesEasy\\Xpos\\XposServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}